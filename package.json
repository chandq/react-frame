  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=12.0.0"
  }
